#Keycloak Datasource connection and settings

KEYCLOAK_JDBC_DB={{ keycloak.database.database }}
KEYCLOAK_JDBC_USER={{ keycloak.database.username }}
KEYCLOAK_JDBC_PASSWORD={{ keycloak_database_password }}

#Keycloak admin credentials

KEYCLOAK_ADMIN_USER={{ keycloak.admin_username }}
KEYCLOAK_ADMIN_PASSWORD={{ keycloak_admin_password }}
