[all]
hostname ansible_host=37.27.192.122 ansible_user=root ansible_port=22

[all:vars]
keycloak_url=https://keycloak.formsflow.doma.in
formio_url=https://forms.formsflow.doma.in
web_url=https://web.formsflow.doma.in
redash_url=https://redash.formsflow.doma.in
formsflow_api_url=https://api.formsflow.doma.in
camunda_url=https://bpm.formsflow.doma.in
data_analysis_api_url=https://data-analysis.formsflow.doma.in
documents_api_url=https://documents-api.formsflow.doma.in
nocodb_url=https://nocodb.formsflow.doma.in

formio_root_email=admin@example.com
redash_admin_email=admin@example.com
redash_organization=your-org-name

nocodb_connection_string="pg://root_db:5432?u=postgres&p=password&d=root_db"

installation_path=$HOME/docker

enable_caddy=true
enable_keycloak=true
enable_data_analysis=true
enable_redash=true
enable_nocodb=true

; if Keycloak is disabled you have to set the following variable
;bpm_clientsecret=your-secret

multi_tenancy=false
formsflow_admin_url=https://admin.formsflow.doma.in
keycloak_client_auth=false
