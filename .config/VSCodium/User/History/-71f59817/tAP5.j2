###############################################################################
#  This file is a sample file, for Docker compose to work with the settings   #
#  rename this file to .env                                                   #
#  Uncomment the variables if any changes from the default values             #
###############################################################################



#Keycloak Datasource connection and settings

KEYCLOAK_JDBC_DB={{ keycloak.database.database }}
KEYCLOAK_JDBC_USER={{ keycloak.database.username }}
KEYCLOAK_JDBC_PASSWORD={{ keycloak_database_password }}

#Keycloak admin credentials

KC_BOOTSTRAP_ADMIN_USERNAME={{ keycloak.admin_username }}
KC_BOOTSTRAP_ADMIN_PASSWORD={{ keycloak_admin_password }}

KEYCLOAK_START_MODE=start-dev
KEYCLOAK_HTTP_PATH=/auth
